{
    "name": "fomvasss/laravel-medialibrary-extension",
    "description": "Extension for use spatie/laravel-medialibrary",
    "keywords": [
        "laravel-medialibrary",
        "extension",
        "media",
        "conversion",
        "images",
        "laravel"
    ],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^6.18|^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-medialibrary": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\MediaLibraryExtension\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\MediaLibraryExtension\\ServiceProvider"
            ],
            "aliases": {
                "MediaManager": "Fomvasss\\MediaLibraryExtension\\Facade"
            }
        }
    }
}
